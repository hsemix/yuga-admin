{
    "name": "yuga/admin",
    "description": "Administration for your application",
    "keywords": ["framework", "Yuga", "Admin"],
    "type": "yuga-module",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yuga\\Admin\\": "src/Admin/"
        }
    },
    "authors": [
        {
            "name": "Hamid Ssemitala",
            "email": "semix.hamidouh@gmail.com"
        }
    ],
    "require": {
        "php": ">= 8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "yuga/framework": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "yuga": {
            "providers": [
                "Yuga\\Admin\\Providers\\AdminServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
